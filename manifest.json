{
  "name": "TRASLA",
  "id": "eventos.trasla.com.ar",
  "start_url": "/",
  "display": "standalone",
  "description": "La agenda cultural de Traslasierra.",
  "lang": "es-AR",
  "dir": "auto",
  "theme_color": "#E6DECA",
  "background_color": "#E6DECA",
  "orientation": "portrait",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "https",
      "url": "%s",
      "handles": ["trasla.com.ar", "eventos.trasla.com.ar"]
    }
  ],
  "icons": [
    {
      "src": "/assets/icons/favicon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/assets/icons/favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "description",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  }
}
