{
  "name": "TRASLA",
  "id": "eventos.trasla.com.ar",
  "start_url": "https://eventos.trasla.com.ar/",
  "display": "standalone",
  "description": "La agenda cultural de Traslasierra.",
  "lang": "es-AR",
  "dir": "auto",
  "theme_color": "#303030",
  "background_color": "#303030",
  "orientation": "portrait",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "https",
      "url": "%s",
      "handles": ["trasla.com.ar", "eventos.trasla.com.ar"]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://eventos.trasla.com.ar/manifest.json"
    }
  ],
  "icons": [
    {
      "src": "/assets/icons/favicon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/assets/icons/favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*"]
        }
      ]
    }
  }
}
